{
	"cli": {
		"version": ">= 7.5.0",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"ios": {
				"simulator": true
			},
			"env": {
				"EXPO_PUBLIC_R2_PUBLIC_URL": "https://cdn.recordscratch.app",
				"EXPO_PUBLIC_CF_PAGES_URL": "https://recordscratch.app",
				"EXPO_PUBLIC_URL": "recordscratch://",
				"EXPO_PUBLIC_CF_PAGES_URL_ANDROID": "https://recordscratch.app",
				"EXPO_PUBLIC_CF_PAGES_URL_IOS": "https://recordscratch.app"
			}
		},
		"preview": {
			"distribution": "internal"
		},
		"production": {
			"env": {
				"EXPO_PUBLIC_R2_PUBLIC_URL": "https://cdn.recordscratch.app",
				"EXPO_PUBLIC_CF_PAGES_URL": "https://recordscratch.app",
				"EXPO_PUBLIC_URL": "recordscratch://",
				"EXPO_PUBLIC_CF_PAGES_URL_ANDROID": "https://recordscratch.app",
				"EXPO_PUBLIC_CF_PAGES_URL_IOS": "https://recordscratch.app"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
