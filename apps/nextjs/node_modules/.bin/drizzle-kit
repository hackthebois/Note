#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fil/Note/node_modules/.pnpm/drizzle-kit@0.19.13/node_modules/drizzle-kit/node_modules:/home/fil/Note/node_modules/.pnpm/drizzle-kit@0.19.13/node_modules:/home/fil/Note/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fil/Note/node_modules/.pnpm/drizzle-kit@0.19.13/node_modules/drizzle-kit/node_modules:/home/fil/Note/node_modules/.pnpm/drizzle-kit@0.19.13/node_modules:/home/fil/Note/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/drizzle-kit@0.19.13/node_modules/drizzle-kit/index.cjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/drizzle-kit@0.19.13/node_modules/drizzle-kit/index.cjs" "$@"
fi
